<br><div class="col-md-12">
  <div class="panel panel-primary">
    <div class="panel-heading">
      Users wifi Rating
    </div>
    <div class="panel-body">
      <div class="dataTable_wrapper">
        <table class="table table-striped table-bordered table-hover" id="dataTables-example">
          <thead>
            <tr role="row">
              <th class="sorting_asc">No </th>
              <th class="sorting_asc">User Name</th>
              <th>Date</th>
              <th>Wifi Rating</th>
              <th>Wifi Name</th>
            </tr>
          </thead>
          <tbody>
            <% @wifichecks.each_with_index do |wificheck,index| %>
            <tr>
              <td> <%= index+1 %> </td>
              <% @users.each do |user| %><% end %>
              <td><%= wificheck.user.email %></td>
              <td><%= wificheck.to_date %></td>
              <% @ratings.each do |rating| %><% end %>
              <td><%= wificheck.rating.name %></td>
              <% @routers.each do |router| %>  <% end %>
              <td><%= wificheck.router.name %></td>
            </tr>
            <% end %>
          </tbody>
        </table>
      </div>
      <%= link_to 'Back', wifitrackers_new_path, :class =>"btn btn-large btn-primary btn-back"%>
    </div>
  </div>
</div>


